{
  "name": "temp",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "dotenv -e .env.test -- vitest",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules ./src/common/infrastructure/http/server.ts",
    "lint": "eslint .",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:dev": "npm run typeorm -- -d ./src/common/infrastructure/typeorm/config/dataSource.ts",
    "typeorm:migration": "ts-node-esm -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "typeorm:migration:dev": "NODE_ENV=development npm run typeorm:migration -- -d ./src/common/infrastructure/typeorm/config/dataSource.ts",
    "typeorm:migration:test": "NODE_ENV=test npm run typeorm:migration -- -d ./src/common/infrastructure/typeorm/config/testingDataSource.ts",
    "pretest:int": "clear && docker run --rm --name testdb -e POSTGRES_PASSWORD=postgres -p 5433:5432 -d postgres",
    "test:int": "sleep 5 && vitest run --mode test --config ./vitest.int.config.ts",
    "posttest:int": "docker stop testdb"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.26",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@faker-js/faker": "^10.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.3.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "openapi-types": "^12.1.3",
    "prettier": "3.3.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
